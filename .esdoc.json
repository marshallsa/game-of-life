{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.(js|vue)$"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {"accessor": {"access": ["public", "protected"]}}
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "classProperties": true,
        "decorators": true
      }
    },
    {
      "name": "esdoc-vue-plugin"
    }
  ]
}
